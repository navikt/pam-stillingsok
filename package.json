{
    "name": "pam-stillingsok-frontend",
    "version": "0.211.0",
    "description": "",
    "type": "module",
    "packageManager": "pnpm@10.24.0",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "next": "pnpm run dev",
        "start": "next start",
        "build": "NEXT_PUBLIC_SENTRY_RELEASE=$SENTRY_RELEASE next build && pnpm run sentry:sourcemaps",
        "build:no-sourcemap": "NEXT_PUBLIC_SENTRY_RELEASE=$SENTRY_RELEASE next build",
        "dev": "next dev -p 3003 | pino-pretty --messageKey=message",
        "test:prepush": "vitest run --reporter=dot",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "start:dependencies": "./start-docker-compose.sh",
        "start:dependencies-with-update-containers": "./update-containers.sh && ./start-docker-compose.sh",
        "start:dependencies-with-local-search": "COMPOSE_PROFILES=local-search,opensearch,kafka ./start-docker-compose.sh",
        "prettier:format": "npx prettier -w src",
        "generate:page-info-config": "tsx scripts/generate-page-info-config.ts",
        "lint": "eslint . --max-warnings=10",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
        "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
        "prepare": "husky",
        "pre-commit": "lint-staged",
        "compileTS": "tsc --noEmit",
        "sentry:sourcemaps": "if [ -z \"$SENTRY_RELEASE\" ]; then echo 'Skipping Sentry sourcemaps: SENTRY_RELEASE not set'; exit 0; else sentry-cli sourcemaps inject --org nav --project pam-stillingsok ./.next && sentry-cli --url https://sentry.gc.nav.no/ sourcemaps upload --org nav --project pam-stillingsok --release=\"$SENTRY_RELEASE\" ./.next; fi"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fortedigital/nextjs-cache-handler": "^3.0.0",
        "@navikt/aksel-icons": "^8.5.0",
        "@navikt/arbeidsplassen-css": "^10.0.8",
        "@navikt/arbeidsplassen-react": "^10.0.6",
        "@navikt/arbeidsplassen-theme": "^10.0.6",
        "@navikt/ds-css": "^8.5.0",
        "@navikt/ds-react": "^8.5.0",
        "@navikt/ds-tokens": "^8.0.1",
        "@navikt/fnrvalidator": "^2.2.1",
        "@navikt/next-logger": "^4.5.1",
        "@navikt/oasis": "^3.8.0",
        "@sentry/browser": "9.47.1",
        "@sentry/nextjs": "9.47.1",
        "@vercel/otel": "1.14.0",
        "date-fns": "3.6.0",
        "html-react-parser": "5.2.17",
        "isomorphic-dompurify": "2.33.0",
        "jose": "5.10.0",
        "next": "15.4.10",
        "next-logger": "^5.0.2",
        "openid-client": "5.7.1",
        "pino": "^10.3.1",
        "prom-client": "15.1.3",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "redis": "5.10.0",
        "server-only": "0.0.1",
        "sharp": "0.34.5",
        "uuid": "13.0.0",
        "winston": "3.18.3",
        "zod": "4.1.13"
    },
    "overrides": {
        "jose": "5.10.0",
        "openid-client": {
            "jose": "5.10.0"
        }
    },
    "devDependencies": {
        "@axe-core/react": "4.11.1",
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.2",
        "@navikt/aksel-stylelint": "^8.5.2",
        "@next/eslint-plugin-next": "15.5.12",
        "@sentry/cli": "2.58.4",
        "@testing-library/jest-dom": "6.9.1",
        "@testing-library/react": "14.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/eslint": "8.56.12",
        "@types/node": "22.19.11",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "8.48.0",
        "@typescript-eslint/parser": "8.48.0",
        "@typescript-eslint/type-utils": "^8.56.0",
        "@typescript-eslint/utils": "^8.55.0",
        "@vitejs/plugin-react": "^4.2.1",
        "axe-core": "4.11.1",
        "eslint": "9.26.0",
        "eslint-config-prettier": "9.1.2",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-unused-imports": "^4.4.1",
        "globals": "^16.5.0",
        "husky": "9.1.7",
        "import-in-the-middle": "1.15.0",
        "jsdom": "24.1.3",
        "lint-staged": "15.5.2",
        "next-router-mock": "^0.9.13",
        "pino-pretty": "^13.1.3",
        "prettier": "3.8.1",
        "require-in-the-middle": "7.5.2",
        "stylelint": "16.26.0",
        "stylelint-config-recommended": "14.0.1",
        "tsx": "^4.21.0",
        "typescript": "5.9.3",
        "vitest": "3.2.4"
    },
    "dependencyNotes": {
        "globals": "Brukes av ESLint flat config til å definere browser/node-globals (window, document, process osv.).",
        "import-in-the-middle": "Brukes av Sentry/OpenTelemetry for server-instrumentering. Må ligge på rot for pnpm/Next 15.",
        "require-in-the-middle": "Som over, men for CommonJS/require. Sikrer at Sentry-instrumentering fungerer."
    },
    "lint-staged": {
        "src/app/\\(artikler\\)/**/page.tsx": [
            "pnpm run generate:page-info-config",
            "git add src/app/(artikler)/pageInfoConfig.generated.ts"
        ],
        "./*/**/*.{js,jsx,ts,tsx,json,yml,yaml}": [
            "npx prettier --write",
            "npx eslint . --fix"
        ],
        "./*/**/*.css": [
            "npx prettier --write",
            "npx stylelint --fix"
        ],
        "*.{json,md}": [
            "npx prettier --write"
        ]
    }
}

{
    "name": "pam-stillingsok-frontend",
    "version": "0.211.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "./runDev.sh",
        "wonderwall": "scripts/wonderwall.sh",
        "start": "concurrently \"npm run wonderwall\" \"npm run start-webpack\" \"npm run start-express\"",
        "clean-build": "rimraf ./dist",
        "build": "npm run clean-build && webpack -p",
        "start-express": "node server/server.js",
        "start-webpack": "webpack --watch --config webpack.config.dev.js",
        "start:dependencies": "docker-compose up --build --remove-orphans mock-oauth2-server redis",
        "docker-build": "npm run build && docker build . -t pam-stillingsok",
        "docker-start": "docker run --name pam-stillingsok -p 8080:8080 -d pam-stillingsok",
        "docker-kill": "npm run docker-stop && npm run docker-rm",
        "docker-rm": "docker rm pam-stillingsok",
        "docker-stop": "docker stop pam-stillingsok",
        "prettier:format": "npx prettier -w src",
        "lint": "next lint",
        "eslint": "eslint \"**/*.+(js|jsx|ts|tsx)\"",
        "eslint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
        "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
        "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
        "prettier:fixjson": "prettier --write \"**/*.+(json|md)\"",
        "prepare": "husky install",
        "pre-commit": "lint-staged"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@amplitude/analytics-browser": "^1.10.3",
        "@navikt/aksel-icons": "^5.11.1",
        "@navikt/arbeidsplassen-css": "^8.2.8",
        "@navikt/arbeidsplassen-react": "^8.2.4",
        "@navikt/arbeidsplassen-theme": "^5.5.8",
        "@navikt/ds-css": "^5.11.1",
        "@navikt/ds-react": "^5.11.1",
        "@sentry/browser": "^6.19.6",
        "@types/react-router-dom": "^4.3.5",
        "body-parser": "^1.19.2",
        "classnames": "^2.3.1",
        "compression": "^1.7.3",
        "date-fns": "^2.30.0",
        "express": "^4.17.3",
        "helmet": "^4.6.0",
        "html-react-parser": "^3.0.15",
        "http-proxy-middleware": "^2.0.6",
        "isomorphic-dompurify": "^1.5.0",
        "jose": "^4.12.1",
        "lodash.throttle": "^4.1.1",
        "mustache-express": "^1.3.1",
        "node-fetch": "^2.6.7",
        "openid-client": "^5.4.0",
        "prettier": "^2.8.7",
        "promise": "^8.1.0",
        "prop-types": "^15.6.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-modal": "^3.14.3",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "request": "^2.88.2",
        "request-promise": "^4.2.6",
        "ua-parser-js": "^0.7.33",
        "universal-cookie": "^4.0.4",
        "uuid": "^9.0.0",
        "whatwg-fetch": "^2.0.4",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/eslint-parser": "^7.22.15",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-react": "^7.22.15",
        "babel-loader": "^8.3.0",
        "concurrently": "^6.2.0",
        "copy-webpack-plugin": "^6.0.0",
        "css-loader": "^1.0.0",
        "eslint": "^8.39.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.2",
        "mini-css-extract-plugin": "^0.11.0",
        "prettier": "^2.8.8",
        "rimraf": "^2.7.1",
        "stylelint": "^15.10.1",
        "stylelint-config-recommended": "^12.0.0",
        "webpack": "^4.47.0",
        "webpack-cli": "^3.3.12",
        "webpack-merge": "^4.2.1"
    },
    "overrides": {
        "webpack@^4.47.0": {
            "terser-webpack-plugin": "^4.2.3"
        }
    },
    "lint-staged": {
        "./*/**/*.{js,jsx,ts,tsx,json,yml,yaml}": [
            "npx prettier --write",
            "npx eslint --fix"
        ],
        "./*/**/*.css": [
            "npx prettier --write",
            "npx stylelint --fix"
        ],
        "*.{json,md}": [
            "npx prettier --write"
        ]
    }
}

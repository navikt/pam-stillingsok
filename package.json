{
    "name": "pam-stillingsok-frontend",
    "version": "0.211.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "next": "npm run dev",
        "build": "next build",
        "dev": "next dev -p 3003",
        "wonderwall": "scripts/wonderwall.sh",
        "start:dependencies": "./start-docker-compose.sh",
        "docker-build": "npm run build && docker build . -t pam-stillingsok",
        "docker-start": "docker run --name pam-stillingsok -p 8080:8080 -d pam-stillingsok",
        "docker-kill": "npm run docker-stop && npm run docker-rm",
        "docker-rm": "docker rm pam-stillingsok",
        "docker-stop": "docker stop pam-stillingsok",
        "prettier:format": "npx prettier -w src",
        "lint": "next lint",
        "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
        "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
        "prepare": "husky install",
        "pre-commit": "lint-staged"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@amplitude/analytics-browser": "^2.5.2",
        "@navikt/aksel-icons": "^5.18.1",
        "@navikt/arbeidsplassen-css": "^8.2.10",
        "@navikt/arbeidsplassen-react": "^8.2.13",
        "@navikt/arbeidsplassen-theme": "^5.5.16",
        "@navikt/ds-css": "^5.18.1",
        "@navikt/ds-react": "^5.18.1",
        "@navikt/oasis": "^3.2.2",
        "@sentry/nextjs": "^7.105.0",
        "date-fns": "^3.3.1",
        "html-react-parser": "^5.1.8",
        "isomorphic-dompurify": "^2.4.0",
        "next": "^14.1.3",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ua-parser-js": "^1.0.37",
        "uuid": "^9.0.1",
        "winston": "^3.12.0"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^14.1.3",
        "@types/node": "20.11.25",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.1.3",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "stylelint": "^16.2.1",
        "stylelint-config-recommended": "^14.0.0"
    },
    "lint-staged": {
        "./*/**/*.{js,jsx,ts,tsx,json,yml,yaml}": [
            "npx prettier --write",
            "npx eslint --fix"
        ],
        "./*/**/*.css": [
            "npx prettier --write",
            "npx stylelint --fix"
        ],
        "*.{json,md}": [
            "npx prettier --write"
        ]
    }
}
